<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Luqman Hussain, Harry Pierson</Authors>
    <Description>Port of H. Dobbertin, A. Bosselaers, B. Preneel, ``RIPEMD-160, a strengthened version of RIPEMD for .NET Standard 2.1</Description>
    <IncludeSymbols>true</IncludeSymbols>	
    <PackageId>DevHawk.RIPEMD160</PackageId>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/devhawk/RIPEMD160</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <RepositoryUrl>https://github.com/devhawk/RIPEMD160</RepositoryUrl>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>	
    <TargetFramework>netstandard2.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All"/>
  </ItemGroup>

</Project>
